<Window x:Class="MattNode.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MattNode"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1920" Background="#FF7893CA" Closed="window_Closed">
    <Grid x:Name="mainGrid" Width="1920" Height="1080">
        <local:MainCanvas x:Name="mainCanvas"/>
        <Button Content="Button" HorizontalAlignment="Left" Height="60" Margin="1660,38,0,0" VerticalAlignment="Top" Width="214" Click="NodeCreateButton_Clicked"/>
        <local:Inspector HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
